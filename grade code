 public class grade {
        private String subject;
        private double score;
    
        public grade(String subject, double score) {
            this.subject = subject;
            this.score = score;
        }
    
        public String getSubject() {
            return subject;
        }
    
        public double getScore() {
            return score;
        }
    
        public String getLetterGrade() {
            if (score >= 90) {
                return "A";
            } else if (score >= 80) {
                return "B";
            } else if (score >= 70) {
                return "C";
